{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "no-empty": ["error", {"allowEmptyCatch":  true}],
        "comma-dangle": [2, "never"],
        "max-len": ["error", {"ignoreComments":  true, "code":  120}]
    }
}
